{
  "compilerOptions": {
    "jsx": "react",
    "module": "es6",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./src/index.tsx"
  ],
  "exclude": [
    "./node_modules/**/*"
  ]
}
